so ~/dots/.nav/keymap.nav
so ~/dots/.nav/syntax.nav
so ~/dots/.nav/colors.nav

fu! pic(name)
  new
  cd $name
  bu term
end

fu! op_before_vid
#if pid in group, dont start
  !mpv_i
  kill %:prev
  return mpv --fs %:fullpath
end

fu! op_after_vid
#if pid in group, dont kill
  !pkill compton
end

op Video "op_before_vid()" "op_after_vid()"
op Music "op_before_vid()" "op_after_vid()"
op Document "mupdf %:fullpath" "echo nop"

new fm
